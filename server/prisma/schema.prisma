// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Visitation {
  id    Int     @id @default(autoincrement())
  name  String
  address String
  hour String
  date  String
  description String
  evaluation Int
}
